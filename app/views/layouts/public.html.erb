        </a>
    </div>
<!-- End Back To Top Button -->
     <!-- Javascript Files
    ================================================== -->
    <!-- initialize jQuery Library -->

        <!-- initialize jQuery Library -->
        <!-- Main jquery -->
            <%= javascript_include_tag "jquery.js"%>
        <!-- Bootstrap jQuery -->
        <%= javascript_include_tag "bootstrap.min.js"%>
        <!-- Owl Carousel -->
        <%= javascript_include_tag "owl.carousel.min.js" %>
        <!-- Isotope -->
        <%= javascript_include_tag "jquery.isotope.js" %>
        <!-- Pretty Photo -->
            <%= javascript_include_tag "jquery.prettyPhoto.js" %>
        <!-- SmoothScroll -->
        <%= javascript_include_tag "smooth-scroll.js" %>
        <!-- Image Fancybox -->
        <%= javascript_include_tag "jquery.fancybox.pack.js?v=2.1.5" %>
        <!-- Counter  -->
        <%= javascript_include_tag "jquery.counterup.min.js" %>
