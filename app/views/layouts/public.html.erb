    ================================================== -->
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
         <!-- CSS
         ================================================== -->
        <!-- Bootstrap -->

        <%= stylesheet_link_tag 'bootstrap.min.css'%>
        
        <!-- Animation -->
        <%= stylesheet_link_tag 'animate.css'%>
        <!-- Owl Carousel -->
        <%= stylesheet_link_tag 'owl.carousel.css'%>
        <%= stylesheet_link_tag 'owl.theme.css'%>
        <!-- Pretty Photo -->
        <%= stylesheet_link_tag 'prettyPhoto.css'%>
        <!-- Main color style -->
        <%= stylesheet_link_tag 'red.css'%>
        <!-- Template styles-->
        <%= stylesheet_link_tag 'custom.css' %>
        <!-- Responsive -->
        <%= stylesheet_link_tag 'responsive.css' %>
        <%= stylesheet_link_tag 'jquery.fancybox.css?v=2.1.5', media: "screen" %>
  
    <%= javascript_include_tag "http://maps.google.com/maps/api/js?sensor=false" %>
    <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Lato:400,300" %>
    <%= stylesheet_link_tag "http://fonts.googleapis.com/css?family=Raleway:400,300,500" %>
    <%= stylesheet_link_tag "https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css"%>
   

    </head>

 <body data-spy="scroll" data-target=".navbar-fixed-top">
     <%= yield %>
     
